{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		},
		"noEmit": true,
		"jsx": "react-jsx",
		"strict": true
	},
	"include": ["src", "global.d.ts", "vite-env.d.ts"],
	"exclude": ["node_modules"]
}
